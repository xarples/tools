{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "allowJs": false,
    "checkJs": false,
    "declaration": true,
    "sourceMap": true,
    "composite": true,
    "removeComments": false,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
